<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.passwordSelectorAsserts">
   <children>
      <Label layoutX="206.0" layoutY="237.0" text="¡Haz acertado!" textAlignment="JUSTIFY">
         <font>
            <Font name="Comic Sans MS" size="23.0" />
         </font>
      </Label>
      <Label layoutX="150.0" layoutY="322.0" prefHeight="23.0" prefWidth="301.0" text="Sabes escribir una contraseña segura">
         <font>
            <Font name="Comic Sans MS" size="16.0" />
         </font>
      </Label>
      <Button fx:id="continueButton" layoutX="186.0" layoutY="396.0" mnemonicParsing="false" onAction="#addPoints">
         <font>
            <Font size="19.0" />
         </font>
         <graphic>
            <ImageView fitHeight="112.0" fitWidth="168.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../Assets/continue_button.png" />
               </image>
            </ImageView>
         </graphic>
      </Button>
      <ImageView fitHeight="172.0" fitWidth="217.0" layoutX="270.0" layoutY="24.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/plus_points.png" />
         </image>
      </ImageView>
      <ImageView fitHeight="150.0" fitWidth="200.0" layoutX="178.0" layoutY="75.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../Assets/assertImage.png" />
         </image>
      </ImageView>
   </children>
</AnchorPane>
